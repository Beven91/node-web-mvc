{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2017",
    "noImplicitAny": false,
    "removeComments": true,
    "esModuleInterop":true,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators":true,
    "outDir": "./dist/"
  },
  "exclude": [
    "node_modules",
  ]
}